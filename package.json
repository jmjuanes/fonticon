{
    "name": "icons",
    "version": "0.0.1",
    "description": "An open source and hand-crafted SVG icons library",
    "private": true,
    "author": "Josemi Juanes",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jmjuanes/icons"
    },
    "bugs": "https://github.com/jmjuanes/icons/issues",
    "homepage": "https://icons.josemi.xyz",
    "scripts": {
        "build": "yarn build:icons",
        "prebuild:icons": "rm -rf icons.json icons && mkdir icons",
        "build:icons": "node ./scripts/build-icons.js",
        "prebuild:docs": "rm -rf www && mkdir www",
        "build:docs": "babel-node --presets '@babel/preset-react' scripts/docs.js",
        "postbuild:docs": "cp node_modules/lowcss/dist/low.css www/ && cp node_modules/highlight.js/styles/nord.css www/highlight.css && cp assets/* www/ && cp -R icons www/",
        "serve": "yarn serve:docs",
        "serve:docs": "stattic --folder www --port 3000"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/node": "^7.20.7",
        "@babel/preset-react": "^7.18.6",
        "@mdx-js/mdx": "^2.3.0",
        "gray-matter": "^4.0.3",
        "highlight.js": "^11.8.0",
        "lowcss": "^0.12.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "stattic": "^0.4.0"
    }
}
